<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="@*com.instagram.android:dimen/fundraiser_sticker_currency_amount_selector_button_radius" />
            <stroke android:width="@*com.instagram.android:dimen/fundraiser_sticker_currency_amount_selector_button_pressed_border_width" android:color="@*com.instagram.android:color/fundraiser_sticker_currency_amount_selector_button_selected_border_color" />
            <solid android:color="@*android:color/button_material_dark" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@*com.instagram.android:dimen/fundraiser_sticker_currency_amount_selector_button_radius" />
            <stroke android:width="@*com.instagram.android:dimen/fundraiser_sticker_currency_amount_selector_button_border_width" android:color="@*com.instagram.android:color/fundraiser_sticker_currency_amount_selector_button_unselected_border_color" />
            <solid android:color="@*android:color/button_material_dark" />
        </shape>
    </item>
</selector>
