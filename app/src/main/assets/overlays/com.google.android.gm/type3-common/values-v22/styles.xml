<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ColoredNavBarGeneralTheme" parent="@*com.google.android.gm:style/BaseGeneralTheme">
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="ColoredNavBarMailActivityTheme" parent="@*com.google.android.gm:style/BaseMailActivityTheme">
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="ComposeThemeBase" parent="@*com.google.android.gm:style/ColoredNavBarGeneralTheme">
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:dropDownListViewStyle">@*com.google.android.gm:style/ComposeDropdownListViewStyle</item>
        <item name="android:colorBackgroundCacheHint">@*android:color/background_dark</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="*com.google.android.gm:popupMenuStyle">@*com.google.android.gm:style/PopupMenu</item>
    </style>
    <style name="SuwThemeGlifV2.Light" parent="@*com.google.android.gm:style/SuwThemeGlif.Light">
        <item name="android:colorBackground">@*com.google.android.gm:color/suw_glif_background_color_light</item>
        <item name="android:editTextStyle">@*com.google.android.gm:style/SuwEditText</item>
        <item name="android:alertDialogTheme">@*com.google.android.gm:style/SuwAlertDialogTheme.Light</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.google.android.gm:suwDividerInsetEnd">@*com.google.android.gm:dimen/suw_layout_margin_sides</item>
        <item name="*com.google.android.gm:suwDividerInsetStart">@*com.google.android.gm:dimen/suw_layout_margin_sides</item>
        <item name="*com.google.android.gm:suwDividerInsetStartNoIcon">@*com.google.android.gm:dimen/suw_layout_margin_sides</item>
        <item name="*com.google.android.gm:suwEditTextBackgroundColor">@*com.google.android.gm:color/suw_glif_edit_text_bg_light_color</item>
        <item name="*com.google.android.gm:suwGlifHeaderGravity">center_horizontal</item>
        <item name="*com.google.android.gm:suwScrollIndicators">bottom|top</item>
        <item name="*com.google.android.gm:suwBackgroundPatterned">false</item>
        <item name="*com.google.android.gm:suwBackgroundBaseColor">@*android:color/background_floating_material_dark</item>
    </style>
</resources>
