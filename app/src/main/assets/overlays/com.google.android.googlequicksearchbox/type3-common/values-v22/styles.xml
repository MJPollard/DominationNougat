<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="OpaHQActivity.Theme.Base" parent="@*com.google.android.googlequicksearchbox:style/Theme.AppCompat.Light">
        <item name="android:activityOpenEnterAnimation">@*com.google.android.googlequicksearchbox:anim/slide_in_bottom_hq</item>
        <item name="android:activityCloseExitAnimation">@*com.google.android.googlequicksearchbox:anim/slide_out_bottom_hq</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.google.android.googlequicksearchbox:windowActionBar">false</item>
        <item name="*com.google.android.googlequicksearchbox:windowNoTitle">true</item>
    </style>
    <style name="StandaloneZeroState.Theme.Base" parent="@*com.google.android.googlequicksearchbox:style/Theme.Velvet">
        <item name="android:backgroundDimAmount">0.58</item>
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:popupMenuStyle">@*com.google.android.googlequicksearchbox:style/Opa.PopupMenu</item>
        <item name="android:textAppearanceLargePopupMenu">@*com.google.android.googlequicksearchbox:style/TextAppearance.AppCompat.Light.Widget.PopupMenu.Large</item>
        <item name="android:textAppearanceSmallPopupMenu">@*com.google.android.googlequicksearchbox:style/TextAppearance.AppCompat.Light.Widget.PopupMenu.Small</item>
        <item name="android:statusBarColor">@*com.google.android.googlequicksearchbox:color/standalone_zs_status_bar</item>
        <item name="android:navigationBarColor">@*com.google.android.googlequicksearchbox:color/standalone_zs_navigation_bar</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="SuwAlertDialogTheme.Light" parent="@*android:style/Theme.DeviceDefault.Dialog.Alert" />
    <style name="SuwThemeGlifV2.Light" parent="@*com.google.android.googlequicksearchbox:style/SuwThemeGlif.Light">
        <item name="android:colorBackground">@*com.google.android.googlequicksearchbox:color/suw_glif_background_color_light</item>
        <item name="android:editTextStyle">@*com.google.android.googlequicksearchbox:style/SuwEditText</item>
        <item name="android:alertDialogTheme">@*com.google.android.googlequicksearchbox:style/SuwAlertDialogTheme.Light</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.google.android.googlequicksearchbox:suwDividerInsetEnd">@*com.google.android.googlequicksearchbox:dimen/suw_glif_margin_sides</item>
        <item name="*com.google.android.googlequicksearchbox:suwDividerInsetStart">@*com.google.android.googlequicksearchbox:dimen/suw_glif_margin_sides</item>
        <item name="*com.google.android.googlequicksearchbox:suwDividerInsetStartNoIcon">@*com.google.android.googlequicksearchbox:dimen/suw_glif_margin_sides</item>
        <item name="*com.google.android.googlequicksearchbox:suwEditTextBackgroundColor">@*com.google.android.googlequicksearchbox:color/suw_glif_edit_text_bg_light_color</item>
        <item name="*com.google.android.googlequicksearchbox:suwGlifHeaderGravity">center_horizontal</item>
        <item name="*com.google.android.googlequicksearchbox:suwScrollIndicators">bottom|top</item>
        <item name="*com.google.android.googlequicksearchbox:suwBackgroundPatterned">false</item>
        <item name="*com.google.android.googlequicksearchbox:suwBackgroundBaseColor">@*android:color/background_floating_material_dark</item>
    </style>
</resources>
