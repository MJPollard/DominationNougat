<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="NSTheme.SplashScreen" parent="@*com.google.android.apps.magazines:style/NSTheme.Light.CustomToolbar">
        <item name="android:windowBackground">@*com.google.android.apps.magazines:drawable/quantum_launchscreen_google_news</item>
        <item name="android:navigationBarColor">@*android:color/primary_text_leanback_light</item>
        <item name="android:statusBarColor">@*android:color/primary_text_leanback_light</item>
        <item name="android:windowShowWallpaper">false</item>
        <item name="android:windowIsTranslucent">false</item>
    </style>
    <style name="Widget.GoogleMaterial.FloatingActionButton" parent="@*com.google.android.apps.magazines:style/Widget.MaterialComponents.FloatingActionButton">
        <item name="*com.google.android.apps.magazines:rippleColor">@*android:color/ripple_material_dark</item>
        <item name="*com.google.android.apps.magazines:elevation">0.0dp</item>
        <item name="*com.google.android.apps.magazines:tint">@*android:color/white</item>
        <item name="*com.google.android.apps.magazines:hoveredFocusedTranslationZ">@*com.google.android.apps.magazines:dimen/google_fab_translation_z_hovered_focused</item>
        <item name="*com.google.android.apps.magazines:pressedTranslationZ">@*com.google.android.apps.magazines:dimen/google_fab_translation_z_pressed</item>
        <item name="*com.google.android.apps.magazines:backgroundTint">@*android:color/button_material_dark</item>
    </style>
</resources>
