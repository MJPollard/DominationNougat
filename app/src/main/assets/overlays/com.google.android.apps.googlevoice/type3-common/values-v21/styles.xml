<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Base.TextAppearance.AppCompat.Inverse" parent="@*android:style/TextAppearance.Material" />
    <style name="Base.TextAppearance.AppCompat.Large.Inverse" parent="@*android:style/TextAppearance.Material.Large" />
    <style name="Base.TextAppearance.AppCompat.Medium.Inverse" parent="@*android:style/TextAppearance.Material.Medium" />
    <style name="Base.TextAppearance.AppCompat.Small.Inverse" parent="@*android:style/TextAppearance.Material.Small" />
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Subtitle.Inverse" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Subtitle" />
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Title" />
    <style name="Base.Theme.GoogleMaterial.Light" parent="@*com.google.android.apps.googlevoice:style/Base.V14.Theme.GoogleMaterial.Light">
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText" parent="@*android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText.Inverse" parent="@*android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabView" parent="@*android:style/Widget.Material.ActionBar.TabView" />
    <style name="Base.Widget.AppCompat.Light.PopupMenu" parent="@*android:style/Widget.Material.PopupMenu" />
    <style name="BaseTheme" parent="@*com.google.android.apps.googlevoice:style/Theme.GoogleMaterial.Dark.NoActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:radioButtonStyle">@*com.google.android.apps.googlevoice:style/RadioButton</item>
        <item name="android:popupMenuStyle">@*com.google.android.apps.googlevoice:style/Widget.GoogleMaterial.PopupMenu</item>
        <item name="android:textDirection">anyRtl</item>
        <item name="android:colorButtonNormal">@*com.google.android.apps.googlevoice:color/app_accent_color</item>
        <item name="*com.google.android.apps.googlevoice:colorPrimaryGoogle">@*com.google.android.apps.googlevoice:color/app_accent_color</item>
        <item name="*com.google.android.apps.googlevoice:colorPrimaryStateGoogle">@*com.google.android.apps.googlevoice:color/app_accent_color</item>
        <item name="*com.google.android.apps.googlevoice:colorPrimaryVariantGoogle">@*com.google.android.apps.googlevoice:color/app_accent_color</item>
        <item name="*com.google.android.apps.googlevoice:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.googlevoice:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.googlevoice:colorAccent">@*com.google.android.apps.googlevoice:color/app_accent_color</item>
        <item name="*com.google.android.apps.googlevoice:preferenceCategoryStyle">@*com.google.android.apps.googlevoice:style/PreferenceCategoryStyle</item>
        <item name="*com.google.android.apps.googlevoice:preferenceStyle">@*com.google.android.apps.googlevoice:style/PreferenceStyle</item>
        <item name="*com.google.android.apps.googlevoice:switchPreferenceCompatStyle">@*com.google.android.apps.googlevoice:style/SwitchPreferenceCompatStyle</item>
        <item name="*com.google.android.apps.googlevoice:snackbarButtonStyle">@*com.google.android.apps.googlevoice:style/SnackbarButtonStyle</item>
    </style>
    <style name="InCallTheme" parent="@*com.google.android.apps.googlevoice:style/Theme.AppCompat">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:navigationBarColor">@*android:color/transparent</item>
    </style>
    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">#ffbbbbbb</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
    </style>
    <style name="ThemeOverlay.GoogleMaterial.Light.BottomSheetDialog" parent="@*com.google.android.apps.googlevoice:style/ThemeOverlay.MaterialComponents.Dialog">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowAnimationStyle">@*com.google.android.apps.googlevoice:style/Animation.MaterialComponents.BottomSheetDialog</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.googlevoice:bottomSheetStyle">@*com.google.android.apps.googlevoice:style/Widget.GoogleMaterial.BottomSheet.Modal</item>
    </style>
    <style name="VoiceBuyFlowCustomTheme">
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>
    </style>
</resources>
