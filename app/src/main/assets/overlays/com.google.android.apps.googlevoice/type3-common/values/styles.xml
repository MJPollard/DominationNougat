<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BaseTheme" parent="@*com.google.android.apps.googlevoice:style/Theme.GoogleMaterial.Dark.NoActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:radioButtonStyle">@*com.google.android.apps.googlevoice:style/RadioButton</item>
        <item name="android:popupMenuStyle">@*com.google.android.apps.googlevoice:style/Widget.GoogleMaterial.PopupMenu</item>
        <item name="*com.google.android.apps.googlevoice:colorSecondary">@*com.google.android.apps.googlevoice:color/app_accent_color</item>
        <item name="*com.google.android.apps.googlevoice:colorSecondaryVariant">@*com.google.android.apps.googlevoice:color/app_accent_color</item>
        <item name="*com.google.android.apps.googlevoice:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.googlevoice:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.googlevoice:colorAccent">@*com.google.android.apps.googlevoice:color/app_accent_color</item>
        <item name="*com.google.android.apps.googlevoice:preferenceCategoryStyle">@*com.google.android.apps.googlevoice:style/PreferenceCategoryStyle</item>
        <item name="*com.google.android.apps.googlevoice:preferenceStyle">@*com.google.android.apps.googlevoice:style/PreferenceStyle</item>
        <item name="*com.google.android.apps.googlevoice:switchPreferenceCompatStyle">@*com.google.android.apps.googlevoice:style/SwitchPreferenceCompatStyle</item>
        <item name="*com.google.android.apps.googlevoice:snackbarButtonStyle">@*com.google.android.apps.googlevoice:style/SnackbarButtonStyle</item>
    </style>
    <style name="BottomNavigation" parent="@*com.google.android.apps.googlevoice:style/Widget.GoogleMaterial.BottomNavigationView">
        <item name="*com.google.android.apps.googlevoice:labelVisibilityMode">selected</item>
        <item name="*com.google.android.apps.googlevoice:backgroundTint">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="ComplexPreferenceScreenTheme">
        <item name="android:textSize">@*com.google.android.apps.googlevoice:dimen/preferences_primary_font_size</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:background">@*android:color/background_dark</item>
    </style>
    <style name="ContactItemText" parent="@*com.google.android.apps.googlevoice:style/BodyStyle">
        <item name="android:textColor">@*android:color/secondary_text_default_material_dark</item>
    </style>
    <style name="ConversationEventListItemName">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="ConversationEventListItemTime">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@*android:color/secondary_text_default_material_dark</item>
    </style>
    <style name="ConversationListItemName">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_marginLeft">@*com.google.android.apps.googlevoice:dimen/avatar_list_item_title_text_left_margin</item>
        <item name="android:layout_marginTop">@*com.google.android.apps.googlevoice:dimen/avatar_margin</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="FloatingActionButtonStyle" parent="@*com.google.android.apps.googlevoice:style/Widget.GoogleMaterial.FloatingActionButton">
        <item name="*com.google.android.apps.googlevoice:rippleColor">@*android:color/ripple_material_dark</item>
        <item name="*com.google.android.apps.googlevoice:tint">@*android:color/white</item>
        <item name="*com.google.android.apps.googlevoice:backgroundTint">@*com.google.android.apps.googlevoice:color/app_accent_color</item>
    </style>
    <style name="HomeTheme" parent="@*com.google.android.apps.googlevoice:style/BaseTheme">
        <item name="android:actionModeBackground">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.apps.googlevoice:windowActionModeOverlay">true</item>
        <item name="*com.google.android.apps.googlevoice:actionModeStyle">@*com.google.android.apps.googlevoice:style/ActionModeStyle</item>
        <item name="*com.google.android.apps.googlevoice:actionModeCloseDrawable">@*com.google.android.apps.googlevoice:drawable/action_mode_close</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
    </style>
    <style name="HomeTheme.Launcher" parent="@*com.google.android.apps.googlevoice:style/HomeTheme">
        <item name="android:windowBackground">@*com.google.android.apps.googlevoice:drawable/quantum_launchscreen_google_voice</item>
	    <item name="android:windowShowWallpaper">false</item>
        <item name="android:windowIsTranslucent">false</item>
    </style>
    <style name="InCallTheme" parent="@*com.google.android.apps.googlevoice:style/Theme.AppCompat">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="MediaView">
        <item name="*com.google.android.apps.googlevoice:gifOverlayIcon">@*com.google.android.apps.googlevoice:drawable/quantum_ic_gif_white_36</item>
        <item name="*com.google.android.apps.googlevoice:emptyViewIcon">@*com.google.android.apps.googlevoice:drawable/ic_missing_photo</item>
        <item name="*com.google.android.apps.googlevoice:errorIcon">@*com.google.android.apps.googlevoice:drawable/quantum_ic_broken_image_white_24</item>
        <item name="*com.google.android.apps.googlevoice:playOverlayIcon">@*com.google.android.apps.googlevoice:drawable/quantum_ic_play_circle_fill_white_48</item>
        <item name="*com.google.android.apps.googlevoice:photosphereOverlayIcon">@*com.google.android.apps.googlevoice:drawable/ov_lightcycle_32</item>
        <item name="*com.google.android.apps.googlevoice:gifOverlayBackgroundColor">@*com.google.android.apps.googlevoice:color/mediaview_gif_overlay_background_color</item>
        <item name="*com.google.android.apps.googlevoice:playOverlayBackgroundColor">@*android:color/background_dark</item>
        <item name="*com.google.android.apps.googlevoice:progressBarColor">@*com.google.android.apps.googlevoice:color/quantum_googblue</item>
        <item name="*com.google.android.apps.googlevoice:progressBarHeight">6.0dip</item>
        <item name="*com.google.android.apps.googlevoice:fadeInAnimationDurationMs">100</item>
        <item name="*com.google.android.apps.googlevoice:fadeOutAnimationDurationMs">80</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.DeviceDefault.Light" />
    <style name="ProgressDialogWithSpinnerOnly" parent="@*android:style/Theme.Holo.Dialog">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="android:windowBackground">@*com.google.android.apps.googlevoice:color/transparent_bg</item>
        <item name="android:alertDialogStyle">@*com.google.android.apps.googlevoice:style/CustomAlertDialogStyle</item>
        <item name="android:textSize">12.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="QualitySurveyTitleStyle">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
    <style name="SearchHistoryItemTest">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:drawableLeft">@*com.google.android.apps.googlevoice:drawable/quantum_ic_history_grey600_24</item>
        <item name="android:drawablePadding">30.0dip</item>
        <item name="android:drawableStart">@*com.google.android.apps.googlevoice:drawable/quantum_ic_history_grey600_24</item>
    </style>
    <style name="SearchHistoryMoreSearches">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="SearchSectionTitle">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@*android:color/secondary_text_default_material_dark</item>
    </style>
    <style name="SignupTheme" parent="@*com.google.android.apps.googlevoice:style/BaseTheme">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="*com.google.android.apps.googlevoice:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.googlevoice:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="ThemeOverlay.AppCompat.Light" parent="@*com.google.android.apps.googlevoice:style/Base.ThemeOverlay.AppCompat.Light">
        <item name="*com.google.android.apps.googlevoice:colorPrimary">@*android:color/primary_material_dark</item>
    </style>
    <style name="ThemeOverlay.GoogleMaterial.Snackbar" parent="">
        <item name="*com.google.android.apps.googlevoice:colorOnSurface">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="VerificationTheme" parent="@*com.google.android.apps.googlevoice:style/BaseTheme">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowNoTitle">true</item>
    </style>
</resources>
