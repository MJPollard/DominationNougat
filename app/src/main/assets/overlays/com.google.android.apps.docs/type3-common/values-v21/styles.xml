        <item name="*com.google.android.apps.docs:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.docs:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.docs:colorAccent">@*com.google.android.apps.docs:color/google_blue500</item>
        <item name="*com.google.android.apps.docs:snackbarStyle">@*com.google.android.apps.docs:style/Widget.GoogleMaterial.Snackbar</item>
        <item name="*com.google.android.apps.docs:snackbarButtonStyle">@*com.google.android.apps.docs:style/Widget.GoogleMaterial.Button.TextButton.Snackbar</item>
    </style>
