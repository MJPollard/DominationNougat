<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme.Launcher" parent="@*com.google.android.apps.translate:style/AppTheme">
	    <item name="android:windowShowWallpaper">false</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@*com.google.android.apps.translate:drawable/quantum_launchscreen_translate</item>
        <item name="*com.google.android.apps.translate:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="Base.Theme.GoogleMaterial.Light" parent="@*com.google.android.apps.translate:style/Base.V14.Theme.GoogleMaterial.Light">
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="OpticsDialogTheme" parent="@*com.google.android.apps.translate:style/Theme.GoogleMaterial.Dark.NoActionBar.Bridge">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowFullscreen">true</item>
        <item name="*com.google.android.apps.translate:colorAccent">@*com.google.android.apps.translate:color/primary_blue</item>
        <item name="*com.google.android.apps.translate:alertDialogTheme">@*com.google.android.apps.translate:style/AlertDialogDarkTheme</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.DeviceDefault.Light" />
    <style name="SimpleWhiteBgTheme" parent="@*com.google.android.apps.translate:style/AppBaseTheme">
        <item name="android:windowBackground">@*android:color/background_dark</item>
    </style>
    <style name="Theme.AppInvite.Preview" parent="@*com.google.android.apps.translate:style/Theme.AppInvite.Preview.Base">
        <item name="android:colorBackground">@*android:color/background_floating_material_dark</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="Theme.AppInvite.Preview.Base" parent="@*android:style/Theme.NoTitleBar" />
    <style name="ThemeOverlay.GoogleMaterial.Light.BottomSheetDialog" parent="@*com.google.android.apps.translate:style/ThemeOverlay.MaterialComponents.Dialog">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowAnimationStyle">@*com.google.android.apps.translate:style/Animation.MaterialComponents.BottomSheetDialog</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.translate:bottomSheetStyle">@*com.google.android.apps.translate:style/Widget.GoogleMaterial.BottomSheet.Modal</item>
        <item name="*com.google.android.apps.translate:materialButtonStyle">@*com.google.android.apps.translate:style/Widget.GoogleMaterial.Button</item>
    </style>
    <style name="WhiteDialogTheme" parent="@*com.google.android.apps.translate:style/Theme.AppCompat.Light.Dialog">
        <item name="*com.google.android.apps.translate:windowActionBar">false</item>
        <item name="*com.google.android.apps.translate:windowNoTitle">true</item>
        <item name="*com.google.android.apps.translate:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.translate:colorPrimaryDark">@*com.google.android.apps.translate:color/grey_status_bar</item>
        <item name="*com.google.android.apps.translate:colorControlNormal">@*com.google.android.apps.translate:color/grey_on_white_toolbar</item>
        <item name="*com.google.android.apps.translate:titleTextColor">@*com.google.android.apps.translate:color/grey_on_white_toolbar</item>
    </style>
    <style name="WhiteTheme" parent="@*com.google.android.apps.translate:style/Theme.AppCompat.Light.NoActionBar">
        <item name="*com.google.android.apps.translate:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.translate:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.translate:colorAccent">@*com.google.android.apps.translate:color/primary_blue</item>
        <item name="*com.google.android.apps.translate:colorControlNormal">@*android:color/secondary_text_default_material_dark</item>
        <item name="*com.google.android.apps.translate:titleTextColor">@*android:color/white</item>
    </style>
    <style name="Widget.Design.BottomSheet.Modal" parent="@*android:style/Widget">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:elevation">@*com.google.android.apps.translate:dimen/design_bottom_sheet_modal_elevation</item>
        <item name="*com.google.android.apps.translate:backgroundTint">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.apps.translate:behavior_peekHeight">auto</item>
        <item name="*com.google.android.apps.translate:behavior_hideable">true</item>
        <item name="*com.google.android.apps.translate:behavior_skipCollapsed">false</item>
        <item name="*com.google.android.apps.translate:shapeAppearance">@null</item>
        <item name="*com.google.android.apps.translate:shapeAppearanceOverlay">@null</item>
        <item name="*com.google.android.apps.translate:enforceMaterialTheme">false</item>
    </style>
    <style name="growthkit_feature_highlight_black">
        <item name="android:textColor">#ffffffff</item>
    </style>
</resources>
