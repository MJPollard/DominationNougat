<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="PhotoViewTheme" parent="@*com.google.android.apps.messaging:style/Theme.AppCompat">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.apps.messaging:windowActionBarOverlay">true</item>
        <item name="*com.google.android.apps.messaging:actionBarStyle">@*com.google.android.apps.messaging:style/AppCompat.ActionBar</item>
        <item name="*com.google.android.apps.messaging:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="RecipientEditTextViewBase">
        <item name="android:textAppearance">?*android:textAppearanceMedium</item>
        <item name="android:background">@null</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@*com.google.android.apps.messaging:dimen/chip_height</item>
        <item name="android:popupBackground">@*android:color/background_floating_material_dark</item>
        <item name="android:inputType">textEmailAddress|textMultiLine</item>
        <item name="android:textDirection">locale</item>
        <item name="android:textAlignment">viewStart</item>
    </style>
    <style name="action_card_h1_immersive_actions" parent="@*com.google.android.apps.messaging:style/action_card_text">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:fontFamily">roboto-regular</item>
    </style>
    <style name="assistant_currency_widget_currency_name">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:paddingLeft">4.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text"></item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:fontFamily">google-sans</item>
    </style>
    <style name="assistant_currency_widget_currency_value">
        <item name="android:textSize">28.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">right</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text"></item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:inputType">numberDecimal</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:fontFamily">google-sans</item>
        <item name="android:backgroundTint">#80000000</item>
    </style>
    <style name="growthkit_feature_highlight_black">
        <item name="android:textColor">#ffffffff</item>
    </style>
</resources>
