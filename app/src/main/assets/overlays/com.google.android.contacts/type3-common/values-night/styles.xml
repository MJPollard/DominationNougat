<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Base.OneGoogle.GoogleMaterial.NavigationView" parent="@*com.google.android.contacts:style/Widget.GoogleMaterial.NavigationView">
        <item name="*com.google.android.contacts:colorAccent">@*android:color/white</item>
    </style>
    <style name="BaseAppTheme" parent="@*com.google.android.contacts:style/Theme.GoogleMaterial.Dark">
        <item name="*com.google.android.contacts:button_bg_disabled">@*android:color/background_floating_material_dark</item>
        <item name="*com.google.android.contacts:button_text_disabled">@*android:color/secondary_text_default_material_dark</item>
        <item name="*com.google.android.contacts:chip_stroke_color">@*android:color/white</item>
        <item name="*com.google.android.contacts:colorAccent">@*com.google.android.contacts:color/google_blue500</item>
        <item name="*com.google.android.contacts:fab_background_tint">@*com.google.android.contacts:color/google_blue500</item>
    </style>
    <style name="BaseContactsDialogTheme" parent="@*com.google.android.contacts:style/Theme.GoogleMaterial.Dark.Dialog.MinWidth">
        <item name="*com.google.android.contacts:colorAccent">@*com.google.android.contacts:color/google_blue500</item>
    </style>
    <style name="ContactsBottomSheetStyle" parent="@*com.google.android.contacts:style/Widget.GoogleMaterial.BottomSheet.Modal">
        <item name="android:backgroundTint">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="OneGoogle.AccountMenu.Chip" parent="@*com.google.android.contacts:style/OneGoogle.Chip">
        <item name="android:textColor">@*android:color/white</item>
        <item name="*com.google.android.contacts:chipBackgroundColor">@*android:color/button_material_dark</item>
        <item name="*com.google.android.contacts:chipStrokeColor">#33ffffff</item>
        <item name="*com.google.android.contacts:rippleColor">@*android:color/ripple_material_dark</item>
    </style>
</resources>
