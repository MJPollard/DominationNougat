<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AllAppsEmptySearchBackground">
        <item name="android:colorForeground">@*com.android.launcher3:color/all_apps_bg_hand_fill</item>
        <item name="android:colorControlHighlight">@*android:color/white</item>
        <item name="android:colorPrimary">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="AllAppsEmptySearchBackground.Dark" parent="@*com.android.launcher3:style/AllAppsEmptySearchBackground">
        <item name="android:colorForeground">@*com.android.launcher3:color/all_apps_bg_hand_fill_dark</item>
        <item name="android:colorControlHighlight">@*android:color/white</item>
        <item name="android:colorPrimary">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="BaseLauncherTheme" parent="@*android:style/Theme.DeviceDefault.Light.NoActionBar">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:colorEdgeEffect">@*android:color/secondary_text_default_material_dark</item>
    </style>
    <style name="BaseLauncherThemeWithCustomAttrs" parent="@*com.android.launcher3:style/BaseLauncherTheme">
        <item name="*com.android.launcher3:allAppsInterimScrimAlpha">46</item>
        <item name="*com.android.launcher3:allAppsNavBarScrimColor">#80000000</item>
        <item name="*com.android.launcher3:allAppsScrimColor">#ea000000</item>
        <item name="*com.android.launcher3:fastscrollerTextColor">#ffffffff</item>
        <item name="*com.android.launcher3:isMainColorDark">true</item>
        <item name="*com.android.launcher3:isWorkspaceDarkText">false</item>
        <item name="*com.android.launcher3:popupColorPrimary">@*android:color/background_floating_material_dark</item>
        <item name="*com.android.launcher3:popupColorSecondary">@*android:color/background_floating_material_dark</item>
        <item name="*com.android.launcher3:popupColorTertiary">#4dffffff</item>
        <item name="*com.android.launcher3:widgetsTheme">@*com.android.launcher3:style/WidgetContainerTheme</item>
        <item name="*com.android.launcher3:workspaceAmbientShadowColor">#33000000</item>
        <item name="*com.android.launcher3:workspaceKeyShadowColor">#44000000</item>
        <item name="*com.android.launcher3:workspaceShadowColor">#b0000000</item>
        <item name="*com.android.launcher3:workspaceStatusBarScrim">@*com.android.launcher3:drawable/workspace_bg</item>
        <item name="*com.android.launcher3:workspaceTextColor">@*android:color/white</item>
    </style>
    <style name="LauncherTheme.Dark" parent="@*com.android.launcher3:style/LauncherTheme">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#ffffffff</item>
        <item name="android:textColorHint">#a0ffffff</item>
        <item name="android:textColorTertiary">#ccffffff</item>
        <item name="android:colorControlHighlight">#a0ffffff</item>
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.android.launcher3:allAppsInterimScrimAlpha">102</item>
        <item name="*com.android.launcher3:allAppsNavBarScrimColor">#80000000</item>
        <item name="*com.android.launcher3:allAppsScrimColor">#ea000000</item>
        <item name="*com.android.launcher3:fastscrollerTextColor">#ffffffff</item>
        <item name="*com.android.launcher3:isMainColorDark">true</item>
        <item name="*com.android.launcher3:popupColorPrimary">@*android:color/background_floating_material_dark</item>
        <item name="*com.android.launcher3:popupColorSecondary">@*android:color/background_floating_material_dark</item>
        <item name="*com.android.launcher3:popupColorTertiary">#4dffffff</item>
        <item name="*com.android.launcher3:widgetsTheme">@*com.android.launcher3:style/WidgetContainerTheme.Dark</item>
    </style>
    <style name="LauncherTheme.Dark.DarkText" parent="@*com.android.launcher3:style/LauncherTheme.Dark">
        <item name="*com.android.launcher3:allAppsInterimScrimAlpha">25</item>
        <item name="*com.android.launcher3:isWorkspaceDarkText">false</item>
        <item name="*com.android.launcher3:workspaceAmbientShadowColor">@*android:color/transparent</item>
        <item name="*com.android.launcher3:workspaceKeyShadowColor">@*android:color/transparent</item>
        <item name="*com.android.launcher3:workspaceShadowColor">@*android:color/transparent</item>
        <item name="*com.android.launcher3:workspaceStatusBarScrim">@null</item>
        <item name="*com.android.launcher3:workspaceTextColor">#ffffffff</item>
    </style>
    <style name="LauncherTheme.DarkText" parent="@*com.android.launcher3:style/LauncherTheme">
        <item name="*com.android.launcher3:allAppsInterimScrimAlpha">128</item>
        <item name="*com.android.launcher3:isWorkspaceDarkText">true</item>
        <item name="*com.android.launcher3:workspaceAmbientShadowColor">@*android:color/transparent</item>
        <item name="*com.android.launcher3:workspaceKeyShadowColor">@*android:color/transparent</item>
        <item name="*com.android.launcher3:workspaceShadowColor">@*android:color/transparent</item>
        <item name="*com.android.launcher3:workspaceStatusBarScrim">@null</item>
        <item name="*com.android.launcher3:workspaceTextColor">#ffffffff</item>
    </style>
    <style name="WidgetContainerTheme" parent="@*android:style/Theme.DeviceDefault.Settings">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="android:textColorSecondary">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:colorEdgeEffect">@*android:color/secondary_text_default_material_dark</item>
    </style>
    <style name="WidgetContainerTheme.Dark" parent="@*com.android.launcher3:style/AppTheme.Dark">
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="android:colorEdgeEffect">@*android:color/secondary_text_default_material_dark</item>
    </style>
</resources>
